{
  "info": {
    "name": "Send OTP Simple",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000"
    },
    {
      "key": "testEmail",
      "value": "test@gmail.com"
    }
  ],
  "item": [
    {
      "name": "Send OTP Register",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"kind\": 1,\n  \"username\": \"testuser\",\n  \"email\": \"{{testEmail}}\",\n  \"password\": \"password123\",\n  \"fullName\": \"Test User\",\n  \"phone\": \"0123456789\",\n  \"groupId\": 1\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/accounts/register",
          "host": ["{{baseUrl}}"],
          "path": ["accounts", "register"]
        }
      }
    }
  ]
}
